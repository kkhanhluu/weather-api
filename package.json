{
  "name": "weather-api",
  "version": "1.0.0",
  "description": "a minimal api to retrieve cities's information and weather in those cities",
  "main": "dist/src/server.js",
  "author": "Khanh Luu",
  "license": "MIT",
  "scripts": {
    "start": "ts-node ./src/server.ts",
    "prestart:dev": "npm run swagger",
    "prebuild": "npm run swagger",
    "start:dev": "ts-node-dev --respawn --transpile-only --inspect=0.0.0.0 ./src/server.ts",
    "build": "tsc",
    "test": "jest --runInBand --detectOpenHandles",
    "lint": "eslint . --ext .js,.ts",
    "lint:fix": "eslint . --ext .js,.ts --fix",
    "format": "prettier **/*.{js,ts,json} --check",
    "format:fix": "prettier **/*.{js,ts,json} --write",
    "swagger": "tsoa spec",
    "lint-staged": "lint-staged"
  },
  "dependencies": {
    "axios": "^0.27.2",
    "debug": "^4.3.4",
    "dotenv": "^16.0.0",
    "express": "^4.18.1",
    "helmet": "^5.0.2",
    "mongoose": "^6.3.2",
    "morgan": "~1.10.0",
    "reflect-metadata": "^0.1.13",
    "swagger-ui-express": "^4.3.0",
    "tsoa": "^4.0.0",
    "typedi": "^0.10.0"
  },
  "devDependencies": {
    "@commitlint/cli": "^16.2.4",
    "@commitlint/config-conventional": "^16.2.4",
    "@types/debug": "^4.1.7",
    "@types/express": "^4.17.13",
    "@types/jest": "^27.5.0",
    "@types/morgan": "^1.9.3",
    "@types/supertest": "^2.0.12",
    "@types/swagger-ui-express": "^4.1.3",
    "@typescript-eslint/eslint-plugin": "^5.21.0",
    "@typescript-eslint/parser": "^5.21.0",
    "eslint": "^8.14.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-import-resolver-typescript": "^2.7.1",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-jest": "^26.1.5",
    "husky": "^7.0.4",
    "jest": "^28.0.3",
    "lint-staged": "^12.4.1",
    "mongodb-memory-server": "^8.5.2",
    "msw": "^0.39.2",
    "prettier": "^2.6.2",
    "supertest": "^6.2.3",
    "ts-jest": "^28.0.1",
    "ts-node": "^10.7.0",
    "ts-node-dev": "^1.1.8",
    "typescript": "^4.6.4"
  }
}
